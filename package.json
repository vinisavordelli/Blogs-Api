{
  "name": "blogs-api",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dotenv": "8.2.0",
    "eslint-config-trybe-backend": "1.0.3",
    "express": "4.17.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "2.1.0",
    "sequelize": "6.3.4",
    "shelljs": "0.8.4"
  },
  "scripts": {
    "prestart": "npx sequelize-cli db:create && npx sequelize-cli db:migrate $",
